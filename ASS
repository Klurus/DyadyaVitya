	cmp	al,7
	;jnz	leftmod006
	lea	di,rgstrfir
	mov	ax,'xb'
	stosw
	jmp	daleemod00

;------------------------------------regleft010
leftmod010:
	cmp	al,0
	jnz	leftmod011
	lea	di,rgstrfir
	mov	ax,'xb'
	stosw
	lea	di,rgstrsec
	mov	ax,'is'
	stosw
	jmp	daleemod01

