	jnz	contmem
	lea	di,rgstr2askin
	mov 	ax,70
	stosb
	jmp	contmem
;--------------------------------------contMEM
contmem:
	shl	bl,4
	mov	al,bl
	cmp	al,0
	jnz	aska01mem
	lea	di,rgstr3askin
	mov 	ax,48
	stosb
	jmp	finishAskamem

aska01mem:
