leftmod013:
	cmp	al,3
	jnz	leftmod014
	lea	di,rgstrfir
	mov	ax,'pb'
	stosw
	lea	di,rgstrsec
	mov	ax,'id'
	stosw
	jmp	daleemod01

leftmod014:
	cmp	al,4
	jnz	leftmod015
